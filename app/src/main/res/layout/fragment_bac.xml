        app:layout_constraintTop_toBottomOf="@+id/checkbox_prim" />

    <CheckBox
        android:id="@+id/checkbox_bac"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:text="@string/bach"
        app:layout_constraintBottom_toBottomOf="@+id/checkbox_eso"
        app:layout_constraintStart_toEndOf="@+id/checkbox_eso" />

    <CheckBox
        android:id="@+id/checkbox_eso"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:text="@string/eso"
