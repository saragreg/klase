        app:layout_constraintTop_toBottomOf="@+id/natura" />

    <ImageButton
        android:id="@+id/gizar"
        android:layout_width="179dp"
        android:layout_height="40dp"
        android:backgroundTint="@color/white"
        android:src="@drawable/btn_asig"
        app:layout_constraintStart_toStartOf="@+id/mate"
        app:layout_constraintTop_toBottomOf="@+id/fiki" />

    <ImageButton
        android:id="@+id/natura"
        android:layout_width="211dp"
        android:layout_height="38dp"
        android:backgroundTint="@color/white"
        android:src="@drawable/btn_asig"
        app:layout_constraintStart_toEndOf="@+id/fiki"
        app:layout_constraintTop_toTopOf="@+id/fiki" />

    <ImageButton
        android:id="@+id/fiki"
        android:layout_width="156dp"
        android:layout_height="38dp"
        android:layout_marginStart="8dp"
