        app:layout_constraintTop_toTopOf="@+id/checkbox_cas" />

    <CheckBox
        android:id="@+id/checkbox_cas"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/cas"
        android:textSize="13dp"
        app:layout_constraintStart_toStartOf="@+id/textView46"
        app:layout_constraintTop_toBottomOf="@+id/textView46" />

    <CheckBox
        android:id="@+id/checkbox_ing"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:text="@string/ingles"
        app:layout_constraintStart_toEndOf="@+id/checkbox_eusk"
        app:layout_constraintTop_toTopOf="@+id/checkbox_eusk" />

    <ImageButton
        android:id="@+id/tic"
        android:layout_width="61dp"
        android:layout_height="40dp"
        android:backgroundTint="@color/white"
        android:src="@drawable/btn_asig"
        app:layout_constraintStart_toEndOf="@+id/gizar"
        app:layout_constraintTop_toTopOf="@+id/gizar" />

    <ImageButton
        android:id="@+id/apoyo"
        android:layout_width="0dp"
        android:layout_height="40dp"
        android:backgroundTint="@color/white"
        android:src="@drawable/btn_asig"
        app:layout_constraintBottom_toBottomOf="@+id/textView44"
        app:layout_constraintEnd_toEndOf="@+id/ing"
